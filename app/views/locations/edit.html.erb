<h1>Edit a location</h1>
  <div>
    <%= form_for(@location) do |f| %>
    <fieldset>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </fieldset>
    <fieldset>
      <%= f.submit %>
    </fieldset>
    <% end %>
  </div>

  <div class="add-new"><%= link_to "Cancel", locations_path %></div>


